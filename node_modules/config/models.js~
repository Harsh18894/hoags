var mongoose = require('mongoose');  

var Schema = mongoose.Schema;  

var userSchema = mongoose.Schema({    
	token : String,     
	name: String,
	email: String,
	mobile: String,  
	hashed_password: String,    
	salt : String,  
	temp_str:String 
});  

mongoose.connect('mongodb://localhost:27017/hoags'); 
module.exports = mongoose.model('users', userSchema);

